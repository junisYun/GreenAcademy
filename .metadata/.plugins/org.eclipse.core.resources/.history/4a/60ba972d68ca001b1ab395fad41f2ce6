
public class Ex06 {
	public static void main(String[] args) {
		int n = 0x0660;
		char[] block = new char[2];
		block[0] = '¡à';
		block[1] = '¡á';
		int mask = 0x8000; // ÀÌÁø¼ö : 1000 0000
		for(int i = 0; i < 16; i++) {
			System.out.print((n & mask) == mask ? block[0] : " ");
			if((i + 1) % 4 == 0) System.out.println();
			mask = mask >> 1;
		}
		System.out.println();
	}
}
