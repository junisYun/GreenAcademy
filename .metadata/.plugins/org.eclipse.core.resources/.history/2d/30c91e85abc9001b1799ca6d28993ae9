package kr.green.ex0610;

public class VarEx {
	int i ;
	boolean gender;
	String name;
	
	public static void main(String[] args) {
		float var;
		VarEx ex = new VarEx();
		System.out.println("  i    = " + ex.i);
		System.out.println("name   = " +ex.name);
		System.out.println("gender = " +ex.gender);
		ex.name = "";
		System.out.println("name   =  " + ex.name);
		String a = ""+7;
		System.out.println(a);
	}
}
