
public class Ex06 {
	public static void main(String[] args) {
		int n = 0x0660;
		int mask = 0x8000; // ÀÌÁø¼ö : 1000 0000
		for(int i = 0; i < 16; i++) {
			System.out.print((n & mask) == mask ? "1" : "0");
			if((i + 1) % 4 == 0) System.out.println();
			mask = mask >> 1;
		}
		System.out.println();
	}
}
